  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for SQLiteSwiftDemo
  pod 'SQLite.swift', :subspecs => ['standalone']
  pod 'sqlite3', :subspecs => ['fts5']
end
